{
  "name": "test-builds",
  "version": "1.0.0",
  "description": "Read the Docs test builds",
  "main": "app.js",
  "scripts": {
    "clean-js": "rm -v docs/_static/*.js; exit 0",
    "clean-css": "rm -v docs/_static/*.css; exit 0",
    "clean": "npm run clean-js && npm run clean-css",
    "build": "npm run clean && webpack --mode=production --progress --colors",
    "dev": "webpack-dev-server --mode=development",
    "lint": "prettier -c *.js *.json",
    "format": "prettier --write *.js *.json",
    "test": "jest src/js/tests/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "create-fomantic-icons": "^1.0.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "imports-loader": "^0.8.0",
    "jest": "^26.4.2",
    "jquery": "^3.4.1",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "matchdep": "~1.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "resolve-url-loader": "^2.2.1",
    "sanitize-html": "^1.23.0",
    "style-loader": "^0.23.1",
    "tslib": "^2.3.1",
    "url-loader": "^0.6.2",
    "visual-dom-diff": "^0.7.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.1",
    "webpack-shell-plugin": "^0.5.0",
    "webpack-watch-files-plugin": "^1.0.3"
  },
  "directories": {
    "doc": "docs"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "globals": {
      "__webpack_public_path__": null
    }
  }
}
